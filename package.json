{
  "name": "datocms-plugin-commercetools-product",
  "homepage": "https://github.com/naren-bellala/commercetools-dato-plugin",
  "version": "0.0.1",
  "description": "A plugin that allows users to search and select Commercetools SKUs",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --port 5000",
    "dist": "NODE_ENV=production webpack --progress",
    "addToProject": "yo datocms-plugin:add-to-project",
    "prepublishOnly": "rimraf lib dist && mkdir dist && npm run dist",
    "lint": "eslint \"./src/*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"./src/*.{js,jsx}\"",
    "test": "echo 1",
    "tunnel": "ngrok http 5000"
  },
  "keywords": [
    "datocms",
    "datocms-plugin",
    "field-editor",
    "string-field",
    "commercetools",
    "dato-plugin"
  ],
  "files": [
    "dist",
    "docs"
  ],
  "author": "Naren Bellala <naren.bellala@amido.com>",
  "license": "ISC",
  "datoCmsPlugin": {
    "title": "commercetools dato plugin",
    "previewImage": "docs/preview.gif",
    "coverImage": "docs/cover.png",
    "entryPoint": "dist/index.html",
    "pluginType": "field_editor",
    "fieldTypes": [
      "string"
    ],
    "parameters": {
      "global": [
        {
          "id": "developmentMode",
          "label": "Deve mode?",
          "type": "boolean",
          "hint": "Enable debug mode"
        },
        {
          "id": "clientId",
          "label": "Commercetools client ID",
          "type": "string",
          "required": true,
          "default": "9aF1OBkOH8e4hiQvAVkEo5L3",
          "hint": "If you do not posses a api key please visit <code>https://docs.commercetools.com/merchant-center/api-clients</code>"
        },
        {
          "id": "clientSecret",
          "label": "Commercetools client secret",
          "type": "string",
          "required": true,
          "default": "<YOUR_SECRET>",
          "hint": "If you do not posses a api key please visit <code>https://docs.commercetools.com/merchant-center/api-clients</code>"
        },
        {
          "id": "projectKey",
          "label": "Commercetools project key",
          "type": "string",
          "required": true,
          "default": "production-store",
          "hint": "If you do not posses a project_key please visit <code>https://docs.commercetools.com/merchant-center/api-clients</code>"
        }
      ],
      "instance": []
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0-beta.54",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "generator-datocms-plugin": "^1.0.24",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "mini-css-extract-plugin": "^0.4.3",
    "ngrok": "^3.2.7",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.2",
    "style-loader": "^0.23.0",
    "svg-url-loader": "^2.3.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10",
    "yo": "^3.1.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@commercetools/api-request-builder": "^5.6.2",
    "@commercetools/sdk-client": "^2.1.2",
    "@commercetools/sdk-middleware-auth": "^6.1.4",
    "@commercetools/sdk-middleware-http": "^6.0.7",
    "@commercetools/sdk-middleware-logger": "^2.1.1",
    "classname": "^0.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "generator-datocms-plugin": "^1.0.22",
    "graphql-tag": "^2.11.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "immer": "^1.7.3",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "prop-types": "^15.6.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "repatch": "^1.3.12",
    "style-loader": "^0.23.1"
  }
}
